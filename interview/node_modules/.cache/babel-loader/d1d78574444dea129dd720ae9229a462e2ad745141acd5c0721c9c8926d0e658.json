{"ast":null,"code":"import { mapGetters } from 'vuex';\nexport default {\n  data() {\n    return {\n      streetNames: [{\n        id: 1,\n        name: 'Street 1'\n      }, {\n        id: 2,\n        name: 'Street 2'\n      }\n      // ... other street objects\n      ],\n\n      selectedStreets: [],\n      itemsPerPage: 10,\n      currentPage: 1\n    };\n  },\n  computed: {\n    ...mapGetters(['getCenter', 'getMarker', 'getPlaces']),\n    rows() {\n      return this.getPlaces;\n    },\n    paginatedStreetNames() {\n      const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n      const endIndex = startIndex + this.itemsPerPage;\n      return this.streetNames.slice(startIndex, endIndex);\n    },\n    totalPages() {\n      return Math.ceil(this.streetNames.length / this.itemsPerPage);\n    },\n    showPagination() {\n      return this.totalPages > 1;\n    }\n  },\n  methods: {\n    previousPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","data","streetNames","id","name","selectedStreets","itemsPerPage","currentPage","computed","rows","getPlaces","paginatedStreetNames","startIndex","endIndex","slice","totalPages","Math","ceil","length","showPagination","methods","previousPage","nextPage"],"sources":["/home/hejingyuan10/pre-int/interview/src/components/CheckTable.vue"],"sourcesContent":["<template>\n    <div>\n      <table>\n        <thead>\n          <tr>\n            <th></th>\n            <th>Street Name</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"street in paginatedStreetNames\" :key=\"street.id\">\n            <td><input type=\"checkbox\" v-model=\"selectedStreets\" :value=\"street\" /></td>\n            <td>{{ street.name }}</td>\n          </tr>\n        </tbody>\n      </table>\n  \n      <div v-if=\"showPagination\">\n        <button @click=\"previousPage\" :disabled=\"currentPage === 1\">Previous</button>\n        <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\">Next</button>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import { mapGetters } from 'vuex';\n  \n  export default {\n    data() {\n      return {\n        streetNames: [\n          { id: 1, name: 'Street 1' },\n          { id: 2, name: 'Street 2' },\n          // ... other street objects\n        ],\n        selectedStreets: [],\n        itemsPerPage: 10,\n        currentPage: 1,\n      };\n    },\n    computed: {\n      ...mapGetters(['getCenter', 'getMarker', 'getPlaces']),\n      rows (){\n        return this.getPlaces\n    },\n      paginatedStreetNames() {\n        const startIndex = (this.currentPage - 1) * this.itemsPerPage;\n        const endIndex = startIndex + this.itemsPerPage;\n        return this.streetNames.slice(startIndex, endIndex);\n      },\n      totalPages() {\n        return Math.ceil(this.streetNames.length / this.itemsPerPage);\n      },\n      showPagination() {\n        return this.totalPages > 1;\n      },\n    },\n    methods: {\n      previousPage() {\n        if (this.currentPage > 1) {\n          this.currentPage--;\n        }\n      },\n      nextPage() {\n        if (this.currentPage < this.totalPages) {\n          this.currentPage++;\n        }\n      },\n    },\n  };\n  </script>\n  "],"mappings":"AAyBE,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,CACX;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAW,CAAC,EAC3B;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAW;MAC1B;MAAA,CACD;;MACDC,eAAe,EAAE,EAAE;MACnBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGR,UAAU,CAAC,CAAC,WAAW,EAAE,WAAW,EAAE,WAAW,CAAC,CAAC;IACtDS,IAAGA,CAAA,EAAI;MACL,OAAO,IAAI,CAACC,SAAQ;IACxB,CAAC;IACCC,oBAAoBA,CAAA,EAAG;MACrB,MAAMC,UAAS,GAAI,CAAC,IAAI,CAACL,WAAU,GAAI,CAAC,IAAI,IAAI,CAACD,YAAY;MAC7D,MAAMO,QAAO,GAAID,UAAS,GAAI,IAAI,CAA